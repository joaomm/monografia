classe ImpressorDePrimos:
	imprimePrimo(n):
		primo = calculaPrimo(n)
		print "----------------------------------"
		print "O primo numero " + n + "eh " + primo
		print "----------------------------------"		

	calculaPrimo(n):
		calculador = CalculadorDoEnesimoPrimo(n)
		return calculador.calcula()


